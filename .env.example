# Trading Mode: paper | live
TRADING_MODE=paper

# Korea Investment & Securities API (Paper Trading)
KIS_PAPER_APP_KEY=PSxxxxxxxxxxxxxxxx
KIS_PAPER_APP_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
KIS_PAPER_ACCOUNT=50012345-01

# Korea Investment & Securities API (Live Trading)
# WARNING: Only fill these if you intend to trade with real money
KIS_LIVE_APP_KEY=
KIS_LIVE_APP_SECRET=
KIS_LIVE_ACCOUNT=

# DART OpenAPI (Korean Financial Statements)
DART_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# SEC EDGAR (US Financial Statements - no API key required)
# User-Agent is required by SEC. Format: "AppName contact@email.com"
SEC_USER_AGENT=TurtleCANSLIM contact@example.com

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/turtle_canslim

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379/0

# Telegram Notifications (Optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
